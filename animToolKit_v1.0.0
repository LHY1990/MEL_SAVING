global proc animWork_v01(){
    
    //이름은 매번 바꿔서 사용할것
    
    if (`window -ex animWindowId`){
        deleteUI -window animWindowId;
    }
    
    
    window -title "AnimToolKit_v1.0.0" animWindowId;
    
    rowColumnLayout -numberOfColumns 1;
    text -label "\nAnimToolKit v 1. 0. 0\n";
    
    //버튼 시작
	
	
	
	rowColumnLayout -numberOfColumns 1;
	checkBox -label "Translate X" -v 1;
	checkBox -label "Translate Y" -v 1;
	checkBox -label "Translate Z" -v 1;

	checkBox -label "Rotate X" -v 1;
	checkBox -label "Rotate Y" -v 1;
	checkBox -label "Rotate Z" -v 1;

	checkBox -label "Scale X" -v 1;
	checkBox -label "Scale Y" -v 1;
	checkBox -label "Scale Z" -v 1;
	//
	
	
	checkBox -label "Visibility" -v 1;

	rowColumnLayout ;
	text -l "\n";
	
    //intFieldGrp -label "인티저 슬라이드"   _startFrame_id;
    button -label "SET KEY        " -bgc .5 .5 .5 ;
    button -label "임시버튼" -bgc .5 .5 .5 ;
    button -label "임시버튼" -bgc .5 .5 .5 ;
    
    
    //버튼완료    
    showWindow animWindowId;
    
}

animWork_v01();
